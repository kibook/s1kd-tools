<dmRefIdent>
<dmCode\ modelIdentCode="EX"\ systemDiffCode="A"\ systemCode="00"
subSystemCode="0"\ subSubSystemCode="0"\ assyCode="00"\ disassyCode="00"
disassyCodeVariant="A"\ infoCode="040"\ infoCodeVariant="A"
itemLocationCode="D"/>
